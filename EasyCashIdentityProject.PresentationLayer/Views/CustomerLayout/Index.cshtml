@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="tr">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Easy Cash Müşteri Paneli</title>

    <link href="/gentela-gh-pages/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="/gentela-gh-pages/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="/gentela-gh-pages/vendors/nprogress/nprogress.css" rel="stylesheet">
    <link href="/gentela-gh-pages/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <link href="/gentela-gh-pages/build/css/custom.min.css" rel="stylesheet">
</head>

<body class="nav-md footer_fixed">
    <div class="container body">
        <div class="main_container">
            @await Component.InvokeAsync("_CustomerLayoutSidebarPartial")
            @await Component.InvokeAsync("_CustomerLayoutHeaderPartial")
            <div role="main">
                @RenderBody()
            </div>
            <footer>
                <div class="pull-right">
                    Easy Cash Müşteri Panalie @DateTime.Now.Year
                </div>
                <div class="clearfix"></div>
            </footer>
        </div>
    </div>
    <script src="/gentela-gh-pages/vendors/jquery/dist/jquery.min.js"></script>
    <script src="/gentela-gh-pages/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="/gentela-gh-pages/vendors/fastclick/lib/fastclick.js"></script>
    <script src="/gentela-gh-pages/vendors/nprogress/nprogress.js"></script>
    <script src="/gentela-gh-pages/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="/gentela-gh-pages/build/js/custom.min.js"></script>
</body>
</html>